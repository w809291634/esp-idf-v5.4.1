{
    "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.o":true,
    "**/*.su":true,
    "**/*.cmd":true,
  },
    "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    },
    "files.associations": {
      "sstream": "c",
      "regex": "c",
      "gpio.h": "c",
      "esp_system.h": "c",
      "drv_led.h": "c",
      "mydbg.h": "c",
      "stdint.h": "c",
      "board.h": "c",
      "stdio.h": "c",
      "sdkconfig.h": "c",
      "freertos.h": "c",
      "task.h": "c",
      "timers.h": "c",
      "semphr.h": "c",
      "cstdint": "c",
      "esp_log.h": "c",
      "format": "cpp",
      "stacktrace": "cpp",
      "thread": "cpp",
      "string.h": "c",
      "esp_log_timestamp.h": "c"
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt",    // 默认使用cmd终端
    "terminal.integrated.scrollback": 10000,
    "editor.renderControlCharacters": true,                 //  显示空格 
    "editor.renderWhitespace": "all",
}
  

