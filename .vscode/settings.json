{
    "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.o":true,
    "**/*.su":true,
    "**/*.cmd":true,
  },
    "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    },
    "files.associations": {
      "sstream": "c",
      "regex": "c"
    },
    "terminal.integrated.defaultProfile.windows": "Command Prompt"    // 默认使用cmd终端
}
  

